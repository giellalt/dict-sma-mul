   <e usage="dict">
      <lg>
         <l pos="cs">juhkoe</l>
      </lg>
      <mg>
         <tg xml:lang="nob">
            <t pos="cs">fordi</t>
            <xg>
               <x>Tjidtjie aehtjiem girmie juhkoe raeriem gaavna.</x>
               <xt>Mor skryter av far fordi han finner råd.</xt>
            </xg>
         </tg>
         <tg xml:lang="swe">
            <t pos="cs">fordi_SWE</t>
         </tg>
      </mg>
   </e>
   <e usage="dict">
      <lg>
         <l pos="cs">juktie</l>
      </lg>
      <mg>
         <tg xml:lang="nob">
            <t pos="cs">fordi</t>
            <xg>
               <x>Gåårvedh eensilaakan juktie tjåetskeme ålkene!</x>
               <xt>Kle på deg skikkelig fordi det er kaldt ute!</xt>
            </xg>
         </tg>
         <tg xml:lang="swe">
            <t pos="cs">fordi_SWE</t>
         </tg>
      </mg>
      <mg>
         <tg xml:lang="nob">
            <t pos="cs">slik at</t>
            <xg>
               <x>Bööremes lïegkedidh juktie barkose madtjeles sjïdtedh.</x>
               <xt>Best å hvile seg slik at man blir opplagt til arbeidet.</xt>
            </xg>
         </tg>
         <tg xml:lang="swe">
            <t pos="cs">slik at_SWE</t>
         </tg>
      </mg>
   </e>

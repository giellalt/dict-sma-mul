         <l pos="CS">goske</l>
      </lg>
      <mg>
         <tg xml:lang="nob" tw_id="goske 1489c170-58be-4752-910f-cb5b1e7b21cf">
            <re>om tid</re>
            <t pos="Pr">til</t>
            <xg>
               <x>Manne tjeelim goske eevre sæjloejim.</x>
               <xt>Jeg skrev til jeg ble helt utkjørt.</xt>
            </xg>
         </tg>
         <tg xml:lang="swe">
            <re>om tid_SWE</re>
            <t pos="Pr">til_SWE</t>
         </tg>
      </mg>

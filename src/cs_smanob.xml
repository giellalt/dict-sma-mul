   <e usage="dict">
      <lg>
         <l pos="CS">eannan</l>
      </lg>
      <mg>
         <tg xml:lang="nob" tw_id="eannan 3dddd7d0-4ad0-41e8-a214-7d9c288481db">
            <t pos="CS">innen</t>
            <t pos="CS">før</t>
            <xg>
               <x>Bööremes tjaetsiem jovkedh eannan eevre gojhkelåvva.</x>
               <xt>Det er best å drikke vann før man blir veldig tørst.</xt>
            </xg>
         </tg>
         <tg xml:lang="swe">
            <t pos="CS">innen_SWE</t>
         </tg>
      </mg>
   </e>
   <e usage="dict">

         <l pos="cs">mearan</l>
      </lg>
      <mg>
         <tg xml:lang="nob">
            <t pos="cs">mens</t>
            <xg>
               <x>Manne tjaalam mearan bearkoekrovhte dualta.</x>
               <xt>Jeg skriver mens kjøttgryta koker.</xt>
            </xg>
         </tg>
         <tg xml:lang="swe">

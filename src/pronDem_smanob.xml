            </analysis>
<!--            <analysis ms="Pron_Dem_Sg3_Ess"> -->
<!--               <wordform/>					 -->
<!--            </analysis>						 -->
         </mini_paradigm>
      </lg>
      <mg>
         <tg>
            <t pos="pron" type="dem">den/det</t>
            <xg>
               <x>Mij dïhte lea?</x>
               <xt>Hva er det?</xt>
            </xg>
         </tg>
      </mg>
   </e>
 



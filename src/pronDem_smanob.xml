            </analysis>
<!--            <analysis ms="Pron_Dem_Sg_Ess"> -->
<!--               <wordform/>					 -->
<!--            </analysis>						 -->
         </mini_paradigm>
      </lg>
      <mg>
         <tg>
            <t pos="pron" type="dem">den/det</t>
            <xg>
               <x>Dïhte tjohpe lea dov?</x>
               <xt>Er den lua din?</xt>

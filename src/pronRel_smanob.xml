<!-- Relative pronoun -->

   <e src="nj">
      <lg>
         <l pos="pron" type="rel">mij</l>
         <analysis>Pron_Rel_Sg_Nom</analysis>
         <mini_paradigm>
            <analysis ms="Pron_Rel_Sg_Acc">
               <wordform>maam</wordform>
            </analysis>
            <analysis ms="Pron_Rel_Sg_Gen">
               <wordform>man</wordform>
            </analysis>
            <analysis ms="Pron_Rel_Sg_Ill">
               <wordform>mïsse</wordform>
            </analysis>
            <analysis ms="Pron_Rel_Sg_Ill">
               <wordform>masse</wordform>
            </analysis>
            <analysis ms="Pron_Rel_Sg_Ine">
               <wordform>mesnie</wordform>
            </analysis>
            <analysis ms="Pron_Rel_Sg_Ela">
               <wordform>mestie</wordform>
            </analysis>
            <analysis ms="Pron_Rel_Sg_Com">
               <wordform>mejnie</wordform>
            </analysis>
         </mini_paradigm>
      </lg>
      <mg>
         <tg>
            <t pos="pron">hva</t>
            <t pos="pron">hvilken, hvilket</t>
            <t pos="pron">noen, noe</t>
            <t pos="pron">som</t>
	    <xg>
	      <x>mij dïhte</x>
	      <xt>hva er det?</xt>
	    </xg>
	    <xg>
	      <x>mij darhke daate?</x>
	      <xt>hva er nå dette?</xt>
	    </xg>
	    <xg>
	      <x>saahth mij</x>
	      <xt>hva som helst</xt>
	    </xg>
	    <xg>
	      <x>mij dov faeties</x>
	      <xt>hva er det i veien med deg?</xt>
	    </xg>
	    <xg>
	      <x>mij askede daelie?</x>
	      <xt>hvilken måned er det nå?</xt>
	    </xg>
	    <xg>
	      <x>mij biejjede danbien?</x>
	      <xt>hvilken dag er det i dag?</xt>
	    </xg>
	    <xg>
	      <x>mij biejjede jååktan?</x>
	      <xt>hvilken dag var det i går?</xt>
	    </xg>
	    <xg>
	      <x>mij astojde</x>
	      <xt>tid til</xt>
	    </xg>
	    <xg>
	      <x>Ij leah, daajrah, mij astojde onterdidh.</x>
	      <xt>Du vet, det er ikke tid til å undres.</xt>
	    </xg>
	    <xg>
	      <x>mij deahpadamme (sg)</x>
	      <xt>fakta?</xt>
	    </xg>
	    <xg>
	      <x>ij mij gænnah</x>
	      <xt>ikke noe engang </xt>
	    </xg>
         </tg>
      </mg>
   </e>




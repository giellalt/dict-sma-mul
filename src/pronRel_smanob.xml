<!-- Relative pronoun -->

   <e>
      <lg>
         <l pos="pron" type="rel">mij</l>
         <analysis>Pron_Rel_Sg_Nom</analysis>
         <mini_paradigm>
            <analysis ms="Pron_Rel_Sg_Acc">
               <wordform>maam</wordform>
            </analysis>
            <analysis ms="Pron_Rel_Sg_Gen">
               <wordform>man</wordform>
            </analysis>
            <analysis ms="Pron_Rel_Sg_Ill">
               <wordform>mïsse</wordform>
            </analysis>
            <analysis ms="Pron_Rel_Sg_Ill">
               <wordform>masse</wordform>
            </analysis>
            <analysis ms="Pron_Rel_Sg_Ine">
               <wordform>mesnie</wordform>
            </analysis>
            <analysis ms="Pron_Rel_Sg_Ela">
               <wordform>mestie</wordform>
            </analysis>
            <analysis ms="Pron_Rel_Sg_Com">
               <wordform>mejnie</wordform>
            </analysis>
         </mini_paradigm>
      </lg>
      <mg>
         <tg>
            <t pos="pron">hva</t>
            <t pos="pron">hvilken, hvilket</t>
            <t pos="pron">noen, noe</t>
	    <xg>
	      <x>Mij dïhte?</x>
	      <xt>Hva er det?</xt>
	    </xg>
	    <xg>
	      <x>Saaht mij.</x>
	      <xt>Hva som helst.</xt>
	    </xg>
	    <xg>
	      <x>Mij dov faatoes?</x>
	      <xt>Hva er det i veien med deg?</xt>
	    </xg>
	    <xg>
	      <x>Mij biejjide daen biejjien?</x>
	      <xt>Hvilken dag er det i dag?</xt>
	    </xg>
	    <xg>
	      <x>Ij mij gænnah.</x>
	      <xt>Ikke noe engang.</xt>
	    </xg>
         </tg>
      </mg>
   </e>


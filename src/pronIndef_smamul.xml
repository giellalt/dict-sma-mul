   <e usage="dict">
      <lg>
         <l pos="Pron" type="Indef">muvhte</l>
      </lg>
      <mg>
         <tg xml:lang="nob">
            <t pos="Pron">noen</t>
            <t pos="Pron">en og annen</t>
            <xg>
               <x>muvhten lehkien</x>
               <xt>stedvis</xt>
            </xg>
            <xg>
               <x>Jïh dle muvhth lehkieh biejjie balvi gaskem jæjhta jïh almetjem madtjeldahta.</x>
               <xt>Og så noen steder dukker sola fram mellom skyene og gjør mennesket glad.</xt>
            </xg>
            <xg>
               <x>Provrese jïh provkome sjïelh fihkieh, muvhtijste bovtsh, jïh muvhtijste beetnegh, jïh muvhtijste sïlph.</x>
               <xt>Bruden og brudgommen fikk gaver, av noen fikk de rein, og av noen penger og av noen sølv.</xt>
            </xg>
         </tg>
         <tg xml:lang="swe">
            <t pos="Pron">noen_SWE</t>
         </tg>
      </mg>
   </e>

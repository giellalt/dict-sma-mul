<!-- The Refl intensifier jïjtje, it is outside the Px-ed paradigms -->

 <e>
   <lg>
     <l pos="pron" type="refl">jïjtje</l>
   </lg>
   <mg>
     <tg>
       <t pos="pr">selv</t>
       <xg>
         <x>Biegke jårra gåabph jïjtje sæjhta.</x>
         <xt>Vinden blåser dit den vil selv.</xt>
       </xg>		
     </tg>
   </mg>
 </e>


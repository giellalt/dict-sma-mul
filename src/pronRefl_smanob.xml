<!-- Each person-number combination its own paradigm -->

<!-- Refl Sg1 miniparadigm -->

  <e>
    <lg>
      <l pos="pron" type="refl">jïjtjeme</l>
         <analysis>Pron_Refl_Sg1_Acc</analysis>
         <mini_paradigm>
            <analysis ms="Pron_Refl_Sg1_Gen">
               <wordform>jïjtjene</wordform>
            </analysis>
            <analysis ms="Pron_Refl_Sg1_Ill">
               <wordform>jïjtsanne</wordform>
            </analysis>
            <analysis ms="Pron_Refl_Sg1_Ela">
               <wordform>jïjtjestanne</wordform>
            </analysis>
            <analysis ms="Pron_Refl_Sg1_Com">
               <wordform>jïjtjenæjnene</wordform>
            </analysis>
         </mini_paradigm>
    </lg>
    <mg>
      <tg>
	<t pos="pron">meg selv</t>
	<t pos="pron">min egen</t>
	<xg>
	  <x>Manne jïjtjeme vuajnam.</x>
	  <xt>Jeg ser meg selv.</xt>
	</xg>
      </tg>
    </mg>
  </e>


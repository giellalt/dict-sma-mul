<?xml version="1.0" encoding="UTF-8"?>
<r>
   
<!-- reciprocal pronouns (singular?) -->
   
   <e src="nj">
      <lg>
         <l pos="pron" type="recipr">sinsitniem</l>
         <analysis>Pron_Recipr_Sg_Acc</analysis>
         <mini_paradigm>
            <analysis ms="Pron_Recipr_Sg_Acc">
               <wordform>sinsitniem</wordform>
            </analysis>
            <analysis ms="Pron_Recipr_Sg_Gen">
               <wordform>sinsitnien</wordform>
            </analysis>
            <analysis ms="Pron_Recipr_Sg_Ill">
               <wordform>sinsætnan</wordform>
            </analysis>
            <analysis ms="Pron_Recipr_Sg_Ela">
               <wordform>sinsitneste</wordform>
            </analysis>
            <analysis ms="Pron_Recipr_Sg_Com">
               <wordform>sinsitnine</wordform>
            </analysis>
         </mini_paradigm>
      </lg>
      <mg>
	<tg>
	  <t pos="pron">hverandre</t>
	  <xg>
	    <x>Såemies laantesne almetjh sinsitniem bovvehtjeminie.</x>
	    <xt>I noen land holder folk på å drepe hverandre.</xt>
	  </xg>
	</tg>
      </mg>
   </e>
   
   <!-- reciprocal pronouns, singleton forms --> 
   <e>
     <lg>
       <l pos="pron" type="recipr">sinsitnien</l>
       <lemma_ref>sinsitniem</lemma_ref>
       <analysis>Pron_Recipr_Du_Gen</analysis>
     </lg>
     <mg>
       <tg>
	 <t pos="pron">hverandres</t>
	 <xg>
	   <x>Gabte geajnoeh daerpies, guktie göökte bïjlh maehtieh gaavnedidh jïh sinsitnien baaktoe baahtsedh.</x>
	   <xt>En bred vei er nødvendig, slik at to biler kan møtes og komme forbi hverandre.</xt>
	 </xg>
	 <xg>
	   <x>Sinsitnien juelkide bïssin.</x>
	   <xt>De vasket hverandres føtter.</xt>
	 </xg>
       </tg>
     </mg>
   </e>
   
   <e>
     <lg>
       <l pos="pron" type="recipr">sinsætnan</l>
       <lemma_ref>sinsitniem</lemma_ref>
       <analysis>Pron_Recipr_Du_Ill</analysis>
     </lg>
     <mg>
       <tg>
	 <t pos="pron">til hverandre</t>
	 <xg>
	   <x>Saemieh åvtesne meehtin sinsætnan bïhkedidh gogkoe dam edtjin vaedtsedh</x>
	   <xt>Samer kunne før i tiden foklare til hverandre hvor de skulle gå.</xt>
	 </xg>
	 <xg>
	   <x>Ålma jïh gåmmebe sinsætnan vuartasjægan.</x>
	   <xt>Mannen og kona så på hverandre</xt>
	 </xg>
       </tg>
     </mg>
   </e>
   <e>
     <lg>
       <l pos="pron" type="recipr">sinsitneste</l>
       <lemma_ref>sinsitniem</lemma_ref>
       <analysis>Pron_Recipr_Du_Ela</analysis>
     </lg>
     <mg>
       <tg>
	 <t pos="pron">fra hverandre</t>
	 <xg>
	   <x>Luste almetjidie åadtjodh gaavnedidh, sinsitneste saernieh govledh.</x>
	   <xt>Det er artig for folk å treffes, for å høre nytt fra hverandre.</xt>
	 </xg>
	 <xg>
	   <x>Guhkiem ligan sinsitneste orreme, mohte gieriesvoete lij læjhkan seamma nænnoes.</x>
	   <xt>De to hadde vært lenge borte fra hverandre, men kjærligheten var likevel like sterk.</xt>
	 </xg>
       </tg>
     </mg>
   </e>
   <e>
     <lg>
       <l pos="pron" type="recipr">sinsitnine</l>
       <lemma_ref>sinsitniem</lemma_ref>
       <analysis>Pron_Recipr_Du_Com</analysis>
     </lg>
     <mg>
       <tg>
	 <t pos="pron">med hverandre</t>
	 <xg>
	   <x>Almetjh tjoeverieh åadtjodh gaavnedidh, sinsitnine åahpenidh.</x>
	   <xt>Folk må få møtes, for å bli kjent med hverandre.</xt>
	 </xg>
       </tg>
     </mg>
   </e>
</r>


